{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "jQuery": true
  },
  "ecmaFeatures": {
    "modules": true,
    "classes": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "defaultParams": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "eqeqeq": [2, "smart"]
  }
}
